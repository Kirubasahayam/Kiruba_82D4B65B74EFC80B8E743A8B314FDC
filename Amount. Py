class player:
    def play(self) :
       print("the player is playing cricket ") 
class Batsman (player) :
    def play(self) :
      print(" The batsman is bowling ") 
class Bowler(self) :
    def play(self) :
      print (" the bowler is bowling ") 
batsman=Batsman () 
bowler=Bowler () 
batsman. play () 
bowler. play() 
